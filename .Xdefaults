Xft.dpi: 96.0
!XTerm*faceName: Droid Sans Mono
!XTerm*faceName: Anonymous Pro
XTerm*faceName: Dina
XTerm*faceSize: 11
!XTerm*font: -*-dina-medium-r-*-*-13-*-*-*-*-*-*-*
XTerm*background: black
XTerm*foreground: azure
XTerm*saveLines: 1000
XTerm*jumpScroll: true
XTerm*multiScroll: true
XTerm*scrollTtyOutput: false
XTerm*scrollKey: true
XTerm*visualBell: true
XTerm*termName: xterm-256color
XTerm*loginShell: true
XTerm*geometry: 99x29
#if SCREEN_NUM == 0
Xft.rgba:       rgb
#endif
#if SCREEN_NUM == 1
Xft.rgba:       rgb
#endif

*VT100*translations:    #override   \n\
    Shift <KeyPress>Prior: insert() \n\
    Shift <KeyPress>Next:  insert()
