Xft.dpi: 96.0
*saveLines: 1000
*scrollBar: false
*jumpScroll: true
*skipScroll: true
*scrollTtyOutput: false
*scrollKey: true
*visualBell: true

XTerm*background: black
XTerm*foreground: azure
XTerm*faceName: Dina
XTerm*faceSize: 11
XTerm*termName: xterm-256color
XTerm*loginShell: true
XTerm*geometry: 99x29

*VT100*translations:    #override   \n\
    Shift <KeyPress>Prior: insert() \n\
    Shift <KeyPress>Next:  insert()

URxvt.background: black
URxvt.foreground: azure
URxvt.font: xft:Dina:size=11
URxvt.color12: #4567ed
URxvt.fading: 5
URxvt.scrollTtyKeypress: true
URxvt.keysym.S-Prior: \033[5;2~
URxvt.keysym.S-Next: \033[6;2~
URxvt.colorIT: #ffffcc

#if SCREEN_NUM == 0
Xft.rgba:       rgb
#endif
#if SCREEN_NUM == 1
Xft.rgba:       rgb
#endif

