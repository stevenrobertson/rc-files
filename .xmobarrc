Config {
    font = "Dina-9",
    bgColor = "#222222",
    fgColor = "#bbbbbb",
    position = Top,
    lowerOnStart = False,
    commands = [
        Run Weather "KTPA" ["-t","<tempC>C, <skyCondition>, <rh>%",
                            "-L","24","-H","32","--normal","aquamarine2",
                            "--high","chocolate1","--low","lightskyblue"] 36000,
        Run MultiCpu ["-L","3","-H","50","--normal","green","--high","red",
                      "-W","5","-t","CPU: <autobar>", "-b",".","-f", "|"] 10,
        Run Network "eth0" ["-t","<rx> <tx>","-w","5"] 10,
        Run Memory ["-t","<usedbar> <rest>M"] 10,
        Run Date "%a %b %_d %H:%M:%S" "date" 10,
        Run Music,
        Run StdinReader ],
    sepChar = "%",
    alignSep = "}{",
    template = "%StdinReader% }{  %music%  |  %multicpu%  |  %eth0%  |  %memory%   |  %KTPA%    <fc=#88cc88>%date%</fc>"
}

