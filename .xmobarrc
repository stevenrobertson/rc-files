Config {
    font = "xft:Dina-11",
    bgColor = "#00000000",
    fgColor = "lightskyblue",
    position = Top,
    lowerOnStart = False,
    commands = [
        Run Weather "KTPA" ["-t","<tempC>C, <skyCondition>, <rh>%",
                            "-L","24","-H","32","--normal","aquamarine2",
                            "--high","chocolate1","--low","lightskyblue"] 36000,
        Run MultiCpu ["-L","3","-H","50","--normal","green","--high","red",
                      "-W","5","-t","CPU: <bar0> <bar1> <bar2> <bar3>"] 10,
        Run CpuFreq ["-t","Freq: <cpu0> <cpu1> <cpu2> <cpu3>"] 10,
        Run Network "eth1" ["-t","<rx> <tx>","-w","5"] 10,
        Run Memory ["-t","<usedbar> <rest>M"] 10,
        Run Date "%a %b %_d %H:%M:%S" "date" 10,
        Run Music,
        Run StdinReader ],
    sepChar = "%",
    alignSep = "}{",
    template = "%StdinReader% }{ %music%  |  %multicpu%  %cpufreq%  |  %eth1%  |  %memory%   |  %KTPA%    <fc=#ee9a00>%date%</fc>"
}

