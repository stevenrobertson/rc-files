[extensions]
rebase=
hgext.convert=
hgext.mq=
#hgext.keyword=
hgk=
color=

[alias]
latest = log --limit 10

[diff]
git = True
showfunc = True

[ui]
username = Steven Robertson <steven@strobe.cc>

[merge-tools]
gvim.args = -f -d $local $other
gvim.priority = 1

[keyword]
**.py =

[keywordmaps]
Id = {file|basename} {node|short} {date|utcdate}

[encode]
**.py = pipe: sed 's: *$::'
**.rst = pipe: sed 's: *$::'

%include .hgrc_credentials
