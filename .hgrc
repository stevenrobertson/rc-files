[extensions]
rebase=
hgext.convert=
hgext.mq=
#hgext.keyword=
hgk=
color=
hggit=
hgext.bookmarks=

[alias]
latest = log --rev -10:tip

[diff]
git = True
showfunc = True

[ui]
username = Steven Robertson <steven@strobe.cc>
ignore.global = ~/.hgignore.global

[merge-tools]
gvim.args = -f -d $local $other
gvim.priority = 1

[keyword]
**.py =

[keywordmaps]
Id = {file|basename} {node|short} {date|utcdate}

[web]
port = 8001

%include .hgrc_credentials
