[alias]
    ap = add --patch
    ci = commit
    co = checkout
    cp = checkout --patch
    dc = diff --cached
    dh = diff HEAD
    q = stash
    ss = status -sb -uno
    st = status -sb
[color]
    ui = auto
[core]
    excludesfile = ~/.gitignore.global
[user]
    name = Steven Robertson
    email = steven@strobe.cc
[help]
    autocorrect = -1
[merge]
    tool = gvimdiff
[branch]
	autosetuprebase = always
